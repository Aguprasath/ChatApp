
<div class="container-fluid"><br>
  <%if @searched_users.present? %>
    <h5> Users by your search </h5><br>
    <%@searched_users.each do |user| %>
      <%=render user %><br>
    <% end %>
  <%else %>
    <div>
     <h5>  <%= "No Users Found " %> </h5>
    </div>

  <%end %>
  <%=link_to "Back to users",users_path,class: "btn btn-primary" %>
</div>